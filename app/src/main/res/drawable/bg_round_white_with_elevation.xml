            <solid
                android:color="#ffffff"/>
            <corners
                android:radius="10dp" />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp" />
        </shape>
    </item>
</layer-list>