<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#cab9b9b9"/>
            <corners android:radius="2dp" />
            <corners
                android:radius="10dp" />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
